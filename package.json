{
  "name": "ohash",
  "version": "1.1.4",
  "description": "fast data hashing utils",
  "repository": "unjs/ohash",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./crypto": {
      "node": {
        "types": "./dist/crypto/node/index.d.mts",
        "default": "./dist/crypto/node/index.mjs"
      },
      "default": {
        "types": "./dist/crypto/js/index.d.mts",
        "default": "./dist/crypto/js/index.mjs"
      }
    }
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c src test",
    "lint:fix": "eslint --fix . && prettier -w src test",
    "prepack": "unbuild",
    "release": "pnpm test && changelogen --release --push && pnpm publish",
    "benchmark": "node benchmark/object-hash.mjs",
    "test": "pnpm lint && vitest run && pnpm test:types",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@vitest/coverage-v8": "^3.0.6",
    "benchmark": "^2.1.4",
    "changelogen": "^0.5.7",
    "eslint": "^9.20.1",
    "eslint-config-unjs": "^0.4.2",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.6"
  },
  "packageManager": "pnpm@10.4.1"
}
